{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      },
      "@babel/typescript"
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-classes",
      {
        "loose": true
      }
    ],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
